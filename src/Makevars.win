PKG_CFLAGS = -I. -Ipcre -DHAVE_CONFIG_H -DPHAST_VERSION='"v1.0"' -DRPHAST -DR_LAPACK -DPCRE_STATIC -fno-strict-aliasing
PKG_LIBS = $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)
SRCS = $(wildcard *.c)
PCRE_SRCS = $(wildcard pcre/*.c)
OBJECTS = $(SRCS:.c=.o) $(PCRE_SRCS:.c=.o)

all: $(SHLIB)
$(SHLIB): $(OBJECTS)
